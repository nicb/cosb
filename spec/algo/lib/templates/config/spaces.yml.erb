space:
  identifier: <%= spaces.identifier %>
  loudspeaker_positions:
  <% ndx = 1; spaces.speakers.each do |s| -%>
  <%= ndx -%>: [ <%= s.print_hirez -%> ]
  <% ndx += 1 -%>
<% end -%>
virtual_space:
    width: <%= spaces.width %>
    depth: <%= spaces.depth %>
  reverberation_decay: <%= spaces.reverberation_decay %>
